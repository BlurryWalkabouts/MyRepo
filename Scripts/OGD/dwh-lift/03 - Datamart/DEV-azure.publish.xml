<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>DEV_lift-dw</TargetDatabaseName>
    <DeployScriptFileName>03 - Datamart.sql</DeployScriptFileName>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <DropObjectsNotInSource>True</DropObjectsNotInSource>
    <DoNotDropTables>True</DoNotDropTables>
    <DoNotDropRoleMembership>True</DoNotDropRoleMembership>
    <DoNotDropPermissions>True</DoNotDropPermissions>
    <DoNotDropDatabaseRoles>True</DoNotDropDatabaseRoles>
    <ExcludeLogins>True</ExcludeLogins>
    <ExcludeUsers>True</ExcludeUsers>
    <ExcludeServerRoleMembership>True</ExcludeServerRoleMembership>
    <ExcludeServerRoles>True</ExcludeServerRoles>
    <TargetConnectionString></TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <ExcludeCredentials>True</ExcludeCredentials>
    <ExcludeDatabaseScopedCredentials>True</ExcludeDatabaseScopedCredentials>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="ArchiveServer">
      <Value>ogdw.database.windows.net</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="Archive">
      <Value>DEV_lift-archive</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="MDS">
      <Value>MasterData</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="MDSServer">
      <Value>ogdw.database.windows.net</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>