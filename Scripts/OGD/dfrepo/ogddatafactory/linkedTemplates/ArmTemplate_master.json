{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ogddatafactory"
		},
		"AzureSQLTestDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLTestDB'"
		},
		"blobtestjelinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blobtestjelinkedservice'"
		},
		"BlobDataset_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"BlobDataset_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "adf-data"
		},
		"Gen1DestinationFolder_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Gen1DestinationFolder_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "TestFolder"
		},
		"Gen1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://ogdwdev.azuredatalakestore.net/webhdfs/v1"
		},
		"Gen1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "afca0a52-882c-4fa8-b71d-f6db2e36058b"
		},
		"Gen1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "03d914ad-63f7-4fab-b24e-7b56e7da5a56"
		},
		"Gen1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "OGD-EUW-RGR-PRD-BIR-RAP-01"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSQLTestDB_connectionString": {
						"value": "[parameters('AzureSQLTestDB_connectionString')]"
					},
					"blobtestjelinkedservice_connectionString": {
						"value": "[parameters('blobtestjelinkedservice_connectionString')]"
					},
					"Gen1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('Gen1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"Gen1_properties_typeProperties_tenant": {
						"value": "[parameters('Gen1_properties_typeProperties_tenant')]"
					},
					"Gen1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('Gen1_properties_typeProperties_subscriptionId')]"
					},
					"Gen1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('Gen1_properties_typeProperties_resourceGroupName')]"
					},
					"BlobDataset_properties_typeProperties_fileName": {
						"value": "[parameters('BlobDataset_properties_typeProperties_fileName')]"
					},
					"BlobDataset_properties_typeProperties_folderPath": {
						"value": "[parameters('BlobDataset_properties_typeProperties_folderPath')]"
					},
					"Gen1DestinationFolder_properties_typeProperties_fileName": {
						"value": "[parameters('Gen1DestinationFolder_properties_typeProperties_fileName')]"
					},
					"Gen1DestinationFolder_properties_typeProperties_folderPath": {
						"value": "[parameters('Gen1DestinationFolder_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}